{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "preview",
      "env": {
        "GOOGLE_SERVICES_JSON": "google-services.json"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production",
      "env": {
        "GOOGLE_SERVICES_JSON": "google-services.json"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
